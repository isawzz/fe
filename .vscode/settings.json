{
	"editor.tabSize": 2,
	"editor.insertSpaces": false,
	"editor.codeLens": false,
	"editor.formatOnSave": false,
	"editor.dragAndDrop": false,
	"editor.foldingStrategy": "indentation",
	"editor.glyphMargin": false,
	"editor.gotoLocation.alternativeImplementationCommand": "editor.action.goToReferences",
	"editor.gotoLocation.alternativeReferenceCommand": "editor.action.goToTypeDefinition",
	"editor.tabCompletion": "on",
	"editor.wordBasedSuggestionsMode": "allDocuments",
	"extensions.ignoreRecommendations": true,
  "editor.fontSize": 18,
  "terminal.integrated.fontSize": 22,
  "debug.console.fontSize": 22,
  "git.enableSmartCommit": true,
  "window.zoomLevel": 1,
  "breadcrumbs.enabled": false,
  "editor.minimap.enabled": false,
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "workbench.panel.defaultLocation": "right",
  "workbench.panel.opensMaximized": "never",
  "workbench.editor.enablePreview": false,
	"files.exclude": {
		"node_modules": true,
		"package-lock*": true,
		"package.json": true,
		"jsconfig.json": false,
		".gitignore": false,
		".git": true,
		"**/.idea": true,
		"**/__pycache__": true,
		"**/__init__.py": true,
		"cb1":true,
		"cb2":false,
		"cai":true,
		"cita":true,
		"z0.txt":true,
		"**/degrees*":true,
		"**/maze*":true,
		"**/pyscript*":true,
		"**/tf*":true,
		"**/t0*":false,
		"**/t1*":true,
	},
	"phpfmt.passes": ["PSR2KeywordsLowerCase", "PSR2ModifierVisibilityStaticOrder", "ReindentSwitchBlocks"],
	"phpfmt.exclude": [
		"PSR2SingleEmptyLineAndStripClosingTag",
		"PSR2CurlyOpenNextLine",
		"PSR2LnAfterNamespace",
		"ReindentComments",
		"StripNewlineWithinClassBody"
	],
	"phpfmt.psr2": false,
	"[php]": {
		"editor.formatOnSave": true,
		"editor.tabSize": 2
	},
	"phpfmt.php_bin": "C:\\xampp\\php\\php.exe",
	"phpfmt.indent_with_space": 2,
	"phpfmt.cakephp": true,
	"liveServer.settings.CustomBrowser": "chrome",
	"liveServer.settings.fullReload": true,
	"liveServer.settings.port": 5501,
	"liveServer.settings.useWebExt": true,
	// "liveServer.settings.proxy": {
	// 	"enable": true,
	// 	"baseUri": "/",
	// 	"proxyUri": "http://localhost:8080/aroot/",
	// },
	"editor.bracketPairColorization.enabled": false,
	"[python]": {
		"editor.defaultFormatter": "ms-python.black-formatter",
		"editor.formatOnSave": false,
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
	  "editor.autoIndent":"advanced"
	},
}
